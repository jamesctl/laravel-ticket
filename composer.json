{
    "name": "globit/laravel-ticket",
    "description": "Help your customer easy send request support for your website",
    "keywords": [
        "Globit",
        "laravel",
        "laravel-ticket"
    ],
    "homepage": "https://packagist.org/packages/globit/laravel-ticket",
    "license": "MIT",
    "authors": [
        {
            "name": "jamesctl",
            "email": "jamesctl@globit.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "yajra/laravel-datatables": "^11.0",
        "yajra/laravel-datatables-oracle": "^11"
    },
    "autoload": {
        "psr-4": {
            "Globit\\LaravelTicket\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Globit\\LaravelTicket\\LaravelTicketServiceProvider"
            ]
        }
    },
    "minimum-stability": "1.0.0.x-dev",
    "prefer-stable": true
}
